  "news_query_example": {
    "query": "What's the latest news on TSLA?",
    "agent_response": {
      "response_text": "Here are the latest market news articles for Tesla (TSLA):",
      "query_intent": "news",
      "symbol": "TSLA",
      "widgets": [
        {
          "type": "Card",
          "size": "lg",
          "status": {
            "text": "Live News",
            "icon": "newspaper"
          },
          "children": [
            {
              "type": "Title",
              "value": "Tesla (TSLA) Market News",
              "size": "lg"
            },
            {
              "type": "Divider",
              "spacing": 12
            },
            {
              "type": "ListView",
              "limit": 10,
              "children": [
                {
                  "type": "ListViewItem",
                  "children": [
                    {
                      "type": "Text",
                      "value": "Tesla Q4 Earnings Beat Expectations",
                      "weight": "semibold"
                    },
                    {
                      "type": "Caption",
                      "value": "CNBC • 2 hours ago",
                      "size": "sm"
                    }
                  ],
                  "onClickAction": {
                    "type": "open_article",
                    "payload": {"url": "https://cnbc.com/tesla-earnings"},
                    "handler": "client"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  },
