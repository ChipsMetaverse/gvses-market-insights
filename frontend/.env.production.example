VITE_SUPABASE_URL=your-supabase-url-here
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key-here

# API URLs (Production values - these are overridden in .env.development for local dev)
VITE_WS_URL=wss://gvses-market-insights.fly.dev
VITE_API_URL=https://gvses-market-insights.fly.dev

# WebSocket Relay for Voice Providers
VITE_WEBSOCKET_RELAY_URL=ws://localhost:3004  # Local unified relay
# Production: wss://g-vses.fly.dev/stream

# ElevenLabs (Note: API key is on backend only for security)
VITE_ELEVENLABS_AGENT_ID=your-elevenlabs-agent-id-here

# OpenAI Configuration (for ChatKit and Realtime API)
# Frontend access to backend keys via VITE_ prefix
VITE_OPENAI_API_KEY=your-openai-api-key-here

# ChatKit Workflow Configuration (Agent Builder)
VITE_CHATKIT_WORKFLOW_ID=your-workflow-id-here

# G'sves Assistant Configuration (Responses API)
VITE_GVSES_ASSISTANT_ID=your-assistant-id-here
VITE_USE_GVSES_ASSISTANT=true

# Agents SDK A/B Testing Configuration
# Percentage of traffic to route to SDK endpoint (0-100)
# Start with 0% for safety, gradually increase based on performance
VITE_AGENTS_SDK_PERCENTAGE=0

# Phase 2: Realtime SDK Beta Testing
# Enable the new realtime-sdk provider for power users/beta testers
# Set to false for production safety, true for local testing
VITE_REALTIME_SDK_BETA_ENABLED=false
