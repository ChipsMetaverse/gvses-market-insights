# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_WS_URL=ws://localhost:8000

# Sentry Error Tracking (Optional - Leave empty to disable)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development  # or production, staging
VITE_SENTRY_RELEASE=gvses-frontend@1.0.0

# Phase 1: Structured Chart Command Migration
# Enable structured-first command processing (experimental)
VITE_PREFER_STRUCTURED_CHART_COMMANDS=false
# Enable feature flag debugging in development
VITE_DEBUG_FEATURE_FLAGS=false
